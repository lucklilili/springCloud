		return ysqrq;
	}
	/**
	 * 设置：授权截止时间(YYYYMMDD)
	 */
	public void setSqjzrq(String sqjzrq) {
		this.sqjzrq = sqjzrq;
	}
	/**
	 * 获取：授权截止时间(YYYYMMDD)
	 */
	public String getSqjzrq() {
		return sqjzrq;
	}
	/**
	 * 设置：授权日期
	 */
	public void setSqrq(String sqrq) {
		this.sqrq = sqrq;
	}
	/**
	 * 获取：授权日期
	 */
	public String getSqrq() {
		return sqrq;
	}
	/**
	 * 设置：处理标识:0：初始状态;1：审批通过
	 */
	public void setClbz(String clbz) {
		this.clbz = clbz;
	}
	/**
	 * 获取：处理标识:0：初始状态;1：审批通过
