@Table(name = "device_reg_code")
public class DeviceRegCode implements Serializable {
	private static final long serialVersionUID = 1L;
	
	    //
    @Id
    private String id;
	
	    //主机名称
    @Column(name = "zjmc")
    private String zjmc;
	
	    //机身编号
    @Column(name = "jsbh")
    private String jsbh;
	
	    //网口MAC
    @Column(name = "wkmac")
    private String wkmac;
	
	    //CPU的UUID号
    @Column(name = "uuid")
    private String uuid;
	
	    //纳税人识别号
    @Column(name = "nsrsbh")
    private String nsrsbh;
	
	    //设备编号
    @Column(name = "sbbh")
    private String sbbh;
	
	    //设备类型（1：税控盘阵列，0:云票打印机）
    @Column(name = "sblx")
    private String sblx;
	
	    //申请时间(YYYYMMDDHHmmss)
    @Column(name = "sqsj")
    private String sqsj;
	
	    //授权期限(1,2,3年)
    @Column(name = "sqqx")
    private String sqqx;
	
	    //原授权日期(YYYYMMDD)
    @Column(name = "ysqrq")
    private String ysqrq;
	
	    //授权截止时间(YYYYMMDD)
    @Column(name = "sqjzrq")
    private String sqjzrq;
	
	    //授权日期
    @Column(name = "sqrq")
    private String sqrq;
	
	    //处理标识:0：初始状态;1：审批通过
    @Column(name = "clbz")
    private String clbz;
	
	    //是否申请（N：没有申请，Y：已申请）
    @Column(name = "sfsq")
    private String sfsq;
	
	    //授权标识(N：授权失败，Y：授权成功)
    @Column(name = "sqbz")
    private String sqbz;
	
	    //授权码
    @Column(name = "sqm")
    private String sqm;
	

	/**
	 * 设置：
	 */
	public void setId(String id) {
		this.id = id;
	}
	/**
	 * 获取：
	 */
	public String getId() {
		return id;
	}
	/**
