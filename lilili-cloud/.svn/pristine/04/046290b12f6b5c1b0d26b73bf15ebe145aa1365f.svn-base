package com.bwcloud.finance.center.entity;

import javax.persistence.Column;
import javax.persistence.Id;
import javax.persistence.Table;
import java.io.Serializable;


/**
 * @author dz
 * @version 2018-03-22 11:39:56
 */
@Table(name = "loan_devicet_info")
public class LoanDeviceInfo implements Serializable {
    private static final long serialVersionUID = 1L;

    @Id
    private String id;

    /**
     * 统计年月
     */
    @Column(name = "sksbbh")
    private String sksbbh;

    /**
     * 是否有效标识
     */
    @Column(name = "fpsc_jlrq")
    private String fpscJlrq;

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getSksbbh() {
        return sksbbh;
    }

    public void setSksbbh(String sksbbh) {
        this.sksbbh = sksbbh;
    }

    public String getFpscJlrq() {
        return fpscJlrq;
    }

    public void setFpscJlrq(String fpscJlrq) {
        this.fpscJlrq = fpscJlrq;
    }
}
