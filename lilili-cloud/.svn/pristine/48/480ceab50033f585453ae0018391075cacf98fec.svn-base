    <select id="queryInvoiceList" resultType="com.bwcloud.finance.center.entity.PjJdcFpmx">
        SELECT
        *
        FROM pj_jdc_fpmx a
        WHERE
        a.KPRQ BETWEEN
        CONCAT(DATE_FORMAT(#{start_date},'%Y%m%d'),'000000')
        AND CONCAT(DATE_FORMAT(#{end_date},'%Y%m%d'),'235959')
        AND a.NSRSBH = #{xhdwdm}
        ORDER BY a.KPRQ DESC
    </select>
