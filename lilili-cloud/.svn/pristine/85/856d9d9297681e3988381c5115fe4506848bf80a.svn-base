	    //
    @Id
    private String id;
	
	    //客户姓名
    @Column(name = "cust_name")
    private String custName;
	
	    //起息时间
    @Column(name = "loan_start_date")
    private String loanStartDate;
	
	    //到期日期
    @Column(name = "loan_end_date")
    private String loanEndDate;
	
	    //发放金额
    @Column(name = "loan_amt")
    private String loanAmt;
	
	    //剩余本金
    @Column(name = "rest_amt")
    private String restAmt;
	
	    //还款账号
    @Column(name = "reapy_card_no")
    private String reapyCardNo;
	
	    //贷款利率
    @Column(name = "exec_rate")
    private String execRate;
	
	    //本期还款日期
    @Column(name = "term_date")
    private String termDate;
	
	    //本期期数
    @Column(name = "term_num")
    private String termNum;
	
	    //本期应还本金
    @Column(name = "term_princ")
    private String termPrinc;
	
	    //本期应还利息
    @Column(name = "term_inter")
    private String termInter;
	
	    //本期应还金额
    @Column(name = "tem_amt")
    private String temAmt;
	
	    //创建时间
    @Column(name = "create_time")
    private String createTime;
	
	    //修改日期
    @Column(name = "update_time")
    private String updateTime;
	

