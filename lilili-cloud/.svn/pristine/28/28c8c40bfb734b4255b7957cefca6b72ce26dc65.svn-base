    <select id="queryInvoiceList" resultType="com.bwcloud.finance.center.entity.PjJsfpFpmx">
        SELECT
        *
        FROM pj_jsfp_fpmx a
        WHERE
        a.kprq BETWEEN
        CONCAT(DATE_FORMAT(#{start_date},'%Y%m%d'),'000000')
        AND CONCAT(DATE_FORMAT(#{end_date},'%Y%m%d'),'235959')
        AND a.xhdwdm = #{xhdwdm}
        ORDER BY a.kprq DESC
    </select>

